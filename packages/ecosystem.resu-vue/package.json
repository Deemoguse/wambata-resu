{
	"private": true,
	"name": "@wambata/resu-vue",
	"version": "2.0.0",
	"description": "Lightweight result wrapper for TypeScript with zero overhead and full control.",
	"exports": {
		"./*": {
			"types": "./dist/types/*.d.ts",
			"require": "./dist/cjs/*.js",
			"import": "./dist/mjs/*.js"
		}
	},
	"engines": {
		"node": "*"
	},
	"files": [
		"dist",
		"./README.md"
	],
	"scripts": {
		"build": "vite build",
		"eslint": "eslint **/*.ts --fix --color",
		"release": "npm run build && changeset version && changeset publish"
	},
	"keywords": [
		"vue",
		"resu",
		"result",
		"ok",
		"error",
		"try",
		"flow",
		"typed",
		"type-safe",
		"typescript",
		"functional",
		"zero-runtime",
		"no-magic",
		"error-handling",
		"safe-execution",
		"control-flow",
		"pipe",
		"pipeline",
		"async"
	],
	"author": "Ruslan Radionov <cugune.tyan@gmail.com>",
	"bugs": {
		"url": "https://github.com/Deemoguse/wambata-resu/issues/new/choose",
		"email": "cugune.tyan@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Deemoguse/wambata-resu"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^6.0.0",
		"np": "^10.2.0",
		"vite": "^7.0.5",
		"vite-plugin-dts": "^4.5.4",
		"vue-tsc": "^3.0.2"
	},
	"peerDependencies": {
		"@wambata/resu": "^1.0.0",
		"vue": "^3.5.17"
	},
	"publishConfig": {
		"access": "public"
	},
	"license": "Apache-2.0"
}
